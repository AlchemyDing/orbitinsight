<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.orbitinsight.mapper.SinkFeatureMapper">
    <select id="queryAll" resultType="com.orbitinsight.domain.SinkFeature">
        select *
        from `orbit_insight`.`t_sink_feature`
        where is_deleted = 0
    </select>
</mapper>